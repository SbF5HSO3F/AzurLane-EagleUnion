		<!--拒絕開放邊界請求-->
		<Replace Tag="LOC_DIPLO_REJECT_OPEN_BORDERS_FROM_HUMAN_LEADER_ALLEN_M_SUMNER_DD692_ANY" Language="zh_Hant_HK">
